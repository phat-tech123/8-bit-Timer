:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d7b05a2b4b0 .scope module, "Counter_tb" "Counter_tb" 2 1;
 .timescale 0 0;
v0x5d7b05a3fd80_0 .var "CounterClear", 0 0;
v0x5d7b05a3fe40_0 .var "CounterClock", 0 0;
v0x5d7b05a3ff10_0 .var "CounterEdge", 1 0;
v0x5d7b05a40010_0 .net "Overflow", 0 0, L_0x5d7b05a40200;  1 drivers
v0x5d7b05a400e0_0 .net "TCNT", 7 0, v0x5d7b05a3fad0_0;  1 drivers
S_0x5d7b05a2b640 .scope module, "Counter_u" "Counter" 2 9, 3 1 0, S_0x5d7b05a2b4b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "CounterClock";
    .port_info 1 /INPUT 2 "CounterEdge";
    .port_info 2 /INPUT 1 "CounterClear";
    .port_info 3 /OUTPUT 1 "Overflow";
    .port_info 4 /OUTPUT 8 "TCNT";
P_0x5d7b059ef490 .param/l "BIT_WIDTH" 0 3 2, +C4<00000000000000000000000000001000>;
P_0x5d7b059ef4d0 .param/l "BOTH_EDGES" 1 3 14, C4<11>;
P_0x5d7b059ef510 .param/l "FALLING_EDGE" 1 3 13, C4<10>;
P_0x5d7b059ef550 .param/l "PROHIBITED" 1 3 11, C4<00>;
P_0x5d7b059ef590 .param/l "RISING_EDGE" 1 3 12, C4<01>;
v0x5d7b059ef7a0_0 .net "CounterClear", 0 0, v0x5d7b05a3fd80_0;  1 drivers
v0x5d7b05a3f860_0 .net "CounterClock", 0 0, v0x5d7b05a3fe40_0;  1 drivers
v0x5d7b05a3f920_0 .net "CounterEdge", 1 0, v0x5d7b05a3ff10_0;  1 drivers
v0x5d7b05a3fa10_0 .net "Overflow", 0 0, L_0x5d7b05a40200;  alias, 1 drivers
v0x5d7b05a3fad0_0 .var "TCNT", 7 0;
L_0x7f3ac78ce018 .functor BUFT 1, C4<11111111>, C4<0>, C4<0>, C4<0>;
v0x5d7b05a3fc00_0 .net/2u *"_ivl_0", 7 0, L_0x7f3ac78ce018;  1 drivers
E_0x5d7b059efe80/0 .event negedge, v0x5d7b05a3f860_0;
E_0x5d7b059efe80/1 .event posedge, v0x5d7b059ef7a0_0, v0x5d7b05a3f860_0;
E_0x5d7b059efe80 .event/or E_0x5d7b059efe80/0, E_0x5d7b059efe80/1;
L_0x5d7b05a40200 .cmp/eq 8, v0x5d7b05a3fad0_0, L_0x7f3ac78ce018;
    .scope S_0x5d7b05a2b640;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5d7b05a3fad0_0, 0, 8;
    %end;
    .thread T_0;
    .scope S_0x5d7b05a2b640;
T_1 ;
    %wait E_0x5d7b059efe80;
    %load/vec4 v0x5d7b059ef7a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5d7b05a3fad0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5d7b05a3f920_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x5d7b05a3fad0_0;
    %assign/vec4 v0x5d7b05a3fad0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x5d7b05a3f860_0;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_1.6, 9;
    %load/vec4 v0x5d7b05a3f920_0;
    %pushi/vec4 1, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_1.6;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %load/vec4 v0x5d7b05a3fad0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5d7b05a3fad0_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x5d7b05a3f860_0;
    %nor/r;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_1.9, 9;
    %load/vec4 v0x5d7b05a3f920_0;
    %pushi/vec4 2, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_1.9;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.7, 8;
    %load/vec4 v0x5d7b05a3fad0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5d7b05a3fad0_0, 0;
    %jmp T_1.8;
T_1.7 ;
    %load/vec4 v0x5d7b05a3f920_0;
    %cmpi/e 3, 0, 2;
    %jmp/0xz  T_1.10, 4;
    %load/vec4 v0x5d7b05a3fad0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5d7b05a3fad0_0, 0;
    %jmp T_1.11;
T_1.10 ;
    %load/vec4 v0x5d7b05a3fad0_0;
    %assign/vec4 v0x5d7b05a3fad0_0, 0;
T_1.11 ;
T_1.8 ;
T_1.5 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5d7b05a2b4b0;
T_2 ;
    %vpi_call 2 18 "$dumpfile", "Output/Counter.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5d7b05a2b4b0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5d7b05a2b4b0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d7b05a3fe40_0, 0, 1;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x5d7b05a3ff10_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d7b05a3fd80_0, 0, 1;
    %delay 2000, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5d7b05a2b4b0;
T_4 ;
    %delay 5, 0;
    %load/vec4 v0x5d7b05a3fe40_0;
    %inv;
    %store/vec4 v0x5d7b05a3fe40_0, 0, 1;
    %jmp T_4;
    .thread T_4;
